<h1>place#edit</h1>
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-8">
      <%= simple_form_for(@place, url: account_place_path) do |f| %>
        <%= f.error_notification %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :description %>
        <%= f.input :photo %>
        <%= f.input :nbr_person %>
        <%= f.button :submit %>
      <% end %>
    </div>

    <div class="col-xs-12 col-sm-3">
      <h3>Reservez en un clic votre oenotour</h3>
    </div>
  </div>

</div>
